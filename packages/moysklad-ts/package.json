{
  "name": "moysklad-ts",
  "version": "0.0.5",
  "description": "Fully-typed Moysklad JSON API client",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "moysklad",
    "warehouse",
    "client",
    "wms",
    "crm",
    "api"
  ],
  "homepage": "https://moysklad-ts.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/MonsterDeveloper/moysklad-ts.git"
  },
  "bugs": {
    "url": "https://github.com/MonsterDeveloper/moysklad-ts/issues"
  },
  "license": "GPL-3.0-only",
  "author": "Andrei Bobkov <slider7259@gmail.com>",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts .",
    "typecheck": "vitest typecheck --run"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "msw": "^1.3.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/moysklad-ts.umd.cjs",
  "module": "./dist/moysklad-ts.js",
  "types": "./dist/moysklad-ts.d.ts",
  "exports": {
    ".": {
      "import": "./dist/moysklad-ts.js",
      "require": "./dist/moysklad-ts.umd.cjs",
      "types": "./dist/moysklad-ts.d.ts"
    }
  },
  "dependencies": {
    "type-fest": "^4.3.1",
    "js-base64": "^3.7.5"
  }
}
